{
    "name": "ansible",
    "displayName": "Ansible",
    "description": "Ansible language support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.53.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ansible",
            "aliases": ["Ansible", "ansible"],
            "extensions": [".yml",".yaml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ansible",
            "scopeName": "source.ansible",
            "path": "./syntaxes/ansible.tmLanguage.json"
        }]
    }
}